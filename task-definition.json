{
  "family": "carai-api-task",
  "taskRoleArn": "arn:aws:iam::657214731516:role/CI_CD_ECS_ECR_OIDC_Role",
  "executionRoleArn": "arn:aws:iam::657214731516:role/CI_CD_ECS_ECR_OIDC_Role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "carai-api-container",
      "image": "657214731516.dkr.ecr.us-east-1.amazonaws.com/carai-api:latest",
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/carai-api",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [],
      "volumesFrom": []
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
